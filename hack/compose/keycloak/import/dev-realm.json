{
  "realm": "dev",
  "enabled": true,
  "clients": [
    {
      "clientId": "test-app",
      "name": "Test App",
      "enabled": true,
      "protocol": "openid-connect",

      "publicClient": false,
      "clientAuthenticatorType": "client-secret",
      "secret": "${MY_APP_CLIENT_SECRET}",

      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,

      "redirectUris": ["http://localhost:9000/*"],
      "webOrigins": ["http://localhost:9000"]
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "emailVerified": true,
      "email": "testuser@example.com",
      "credentials": [
        { "type": "password", "value": "testuser", "temporary": false }
      ]
    }
  ]
}
